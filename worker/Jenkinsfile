pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
		sleep 2
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
		sleep 5
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
		sleep 3
            }
        }
    }
    post {
        always {
	    echo 'Finsh-finish-finish'
	}
    }
}
